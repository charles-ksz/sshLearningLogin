<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kevin.entity.Book" table="book" schema="bookmanage">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="native"/>
        </id>
        <property name="name" column="name"/>
        <property name="author" column="author"/>
        <property name="publisher" column="publisher"/>
        <property name="bookid" column="bookid"/>
        <property name="type" column="type"/>
        <property name="allmount" column="allmount"/>
        <property name="mount" column="mount"/>
        <property name="price" column="price"/>
        <property name="remark" column="remark"/>
    </class>
</hibernate-mapping>