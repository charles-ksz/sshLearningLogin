<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kevin.entity.Borrowinfo" table="borrowinfo" schema="bookmanage">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="native"/>
        </id>
        <property name="bookid" column="bookid"/>
        <property name="username" column="username"/>
        <property name="borrowtime" column="borrowtime"/>
        <property name="backtime" column="backtime"/>
        <property name="isback" column="isback"/>
    </class>
</hibernate-mapping>